# This file is used to designate compatibilty with different versions of Course

# Examples
#
# '1.2.x'  => { :branch => 'master' }
# '1.1.x'  => { :branch => '1-1-stable' }
# '1.0.x'  => { :branch => '1-0-stable' }
# '0.70.x' => { :branch => '0-70-stable' }
# '0.40.x' => { :tag => 'v1.0.0', :version => '1.0.0' }
